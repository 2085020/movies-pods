{
  "name": "sample-typescript-pods",
  "version": "1.0.0",
  "description": "In this example we will reorganize the project architecture following a pod approach.",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch webpack.*.js --exec \"webpack-dev-server --config webpack.dev.js\"",
    "start:prod": "nodemon --watch webpack.*.js --exec \"webpack-dev-server --config webpack.prod.js\"",
    "build": "npm run build:dev",
    "build:dev": "rimraf dist && webpack --config webpack.dev.js",
    "build:prod": "rimraf dist && webpack -p --config webpack.prod.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/socket.io-client": "^1.4.32",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.8",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-lint": "^4.5.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "connected-react-router": "^6.0.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "redux-saga": "^0.16.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io": "^2.2.0"
  }
}
